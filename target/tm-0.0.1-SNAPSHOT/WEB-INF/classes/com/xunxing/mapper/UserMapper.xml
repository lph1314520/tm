<mapper namespace="com.xunxing.mapper.UserMapper" >
	
	<select id="getUserByUserAndPwd" resultType="java.util.Map" parameterType="java.lang.String">
		select * from CSFrameworkV45_System.dbo.tb_MyUser where  Account=#{username}  and Password=#{password}
	</select>
	

